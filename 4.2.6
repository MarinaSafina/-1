if (d<0):
   print(" решений нет")
elif (d>0) :
  x1=(-b+ math.sqrt(d))/2*a
  x2=(-b - math.sqrt(d))/2*a
  print('x1= ' ,round(x1, 1), 'x2= ' ,round(x2, 1))
elif (d<0):
   print(" решений нет")
else:
  x=-b/2*a
